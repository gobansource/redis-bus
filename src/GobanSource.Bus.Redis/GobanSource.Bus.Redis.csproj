<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Nullable>enable</Nullable>
    <LangVersion>10.0</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>GobanSource.Bus.Redis</AssemblyName>

    <PackageId>GobanSource.Bus.Redis</PackageId>

    <VersionPrefix>0.1.0</VersionPrefix>
    <VersionSuffix></VersionSuffix>
    <SourceRevisionId>$([System.DateTime]::UtcNow.ToString("yyyyMMddHHmmss"))</SourceRevisionId>
    <Authors>GobanSource</Authors>
    <Description>GobanSource.Bus.Redis is a library for sending and receiving messages between services using Redis.</Description>
    <PackageTags>redis;message;bus</PackageTags>
    <PackageProjectUrl>https://github.com/gobansource/redis-bus</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/gobansource/redis-bus/blob/main/LICENSE</PackageLicenseUrl>

    <RepositoryUrl>https://github.com/gobansource/redis-bus</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="2.8.24" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.2" />
    <PackageReference Include="System.Text.Json" Version="8.0.5" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="8.0.1" />
    <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="8.0.0" />
  </ItemGroup>

</Project>
